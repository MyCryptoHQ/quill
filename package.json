{
  "name": "signer",
  "productName": "signer",
  "version": "1.0.0",
  "description": "A tool to sign transactions with Mnemonic Phrases, Private Keys and Keystore files",
  "main": ".webpack/main",
  "scripts": {
    "start": "cross-env NODE_ENV=development electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "yarn lint:eslint && yarn lint:prettier",
    "lint:eslint": "eslint --ignore-path .gitignore .",
    "lint:eslint:fix": "yarn lint:eslint --fix",
    "lint:prettier": "prettier --check --ignore-path .gitignore \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "prettier --ignore-path .gitignore --write \"**/*.{js,jsx,ts,tsx,json}\"",
    "tsc": "tsc -p ./tsconfig.json",
    "validate": "npm-run-all --parallel lint tsc",
    "test:coverage": "yarn test --coverage",
    "test": "jest --maxWorkers=2 --testPathIgnorePatterns e2e",
    "test:single": "cross-env DEBUG_PRINT_LIMIT=50000 yarn test --coverage=false --watch",
    "test:e2e": "yarn package && cross-env IS_TEST=true jest --maxWorkers=2 e2e",
    "lint:styles": "stylelint ./src/app/**/*.tsx"
  },
  "keywords": [],
  "repository": "https://github.com/MyCryptoHQ/signer.git",
  "author": "MyCryptoHQ",
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "signer"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "port": 3001,
            "mainConfig": "./webpack_config/main.js",
            "renderer": {
              "config": "./webpack_config/renderer.js",
              "entryPoints": [
                {
                  "html": "./src/app/index.html",
                  "js": "./src/app/index.tsx",
                  "name": "main_window",
                  "preload": {
                    "js": "./src/preload.ts"
                  }
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "7.11.6",
    "@babel/preset-env": "7.11.5",
    "@babel/preset-react": "7.10.4",
    "@babel/preset-typescript": "7.10.4",
    "@electron-forge/cli": "6.0.0-beta.53",
    "@electron-forge/maker-deb": "6.0.0-beta.53",
    "@electron-forge/maker-rpm": "6.0.0-beta.53",
    "@electron-forge/maker-squirrel": "6.0.0-beta.53",
    "@electron-forge/maker-zip": "6.0.0-beta.53",
    "@electron-forge/plugin-webpack": "6.0.0-beta.53",
    "@marshallofsound/webpack-asset-relocator-loader": "0.5.0",
    "@sindresorhus/is": "3.1.2",
    "@szmarczak/http-timer": "4.0.5",
    "@testing-library/react": "11.1.1",
    "@testing-library/react-hooks": "3.4.1",
    "@types/jest": "26.0.14",
    "@types/pbkdf2": "3.1.0",
    "@types/react": "16.9.49",
    "@types/react-dom": "16.9.8",
    "@types/react-redux": "7.1.9",
    "@types/react-router-dom": "5.1.6",
    "@types/react-select": "3.0.19",
    "@types/rebass": "4.0.7",
    "@types/redux-mock-store": "1.0.2",
    "@types/sha256": "0.2.0",
    "@types/styled-components": "5.1.4",
    "@types/styled-system": "5.1.10",
    "@types/uuid": "3.4.0",
    "@types/webpack-env": "1.15.3",
    "@types/ws": "7.2.6",
    "@typescript-eslint/eslint-plugin": "4.1.0",
    "@typescript-eslint/parser": "4.1.0",
    "babel-jest": "26.3.0",
    "copy-webpack-plugin": "6.1.1",
    "cross-env": "7.0.2",
    "csp-html-webpack-plugin": "4.0.0",
    "css-loader": "4.2.1",
    "electron": "10.1.1",
    "eslint": "7.6.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jest": "24.0.2",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.21.2",
    "eslint-plugin-react-hooks": "4.1.2",
    "file-loader": "6.2.0",
    "fork-ts-checker-webpack-plugin": "5.0.14",
    "html-webpack-plugin": "4.5.0",
    "husky": "4.2.5",
    "jest": "26.4.2",
    "jest-mock-random": "1.1.1",
    "jest-styled-components": "7.0.3",
    "lint-staged": "10.4.0",
    "node-loader": "1.0.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.1.1",
    "react-select-event": "5.2.0",
    "react-test-renderer": "16.13.1",
    "redux-mock-store": "1.5.4",
    "redux-saga-test-plan": "4.0.1",
    "spectron": "12.0.0",
    "style-loader": "1.2.1",
    "stylelint": "13.7.2",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "ts-loader": "8.0.2",
    "tsconfig-paths-webpack-plugin": "3.3.0",
    "typescript": "4.0.2",
    "typescript-plugin-styled-components": "1.4.4",
    "utility-types": "3.10.0",
    "webpack-merge": "5.1.4"
  },
  "dependencies": {
    "@ethersproject/abstract-provider": "5.1.0",
    "@ethersproject/address": "5.1.0",
    "@ethersproject/bignumber": "5.1.0",
    "@ethersproject/json-wallets": "5.1.0",
    "@ethersproject/transactions": "5.1.0",
    "@ethersproject/units": "5.1.0",
    "@ethersproject/wallet": "5.1.0",
    "@fontsource/lato": "4.2.2",
    "@fontsource/roboto-mono": "4.2.2",
    "@hot-loader/react-dom": "16.13.0",
    "@mycrypto/wallets": "1.0.2",
    "@rebass/forms": "4.0.6",
    "@reduxjs/toolkit": "1.5.0",
    "bignumber.js": "9.0.1",
    "check-password-strength": "2.0.2",
    "connected-react-router": "6.9.1",
    "date-fns": "2.18.0",
    "electron-log": "4.2.4",
    "electron-squirrel-startup": "1.0.0",
    "electron-store": "6.0.1",
    "electron-traywindow-positioner": "1.1.0",
    "ethereum-blockies-base64": "1.0.2",
    "ethereumjs-wallet": "1.0.1",
    "html-to-image": "1.3.24",
    "keytar": "7.6.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-hot-loader": "4.13.0",
    "react-inlinesvg": "2.2.2",
    "react-markdown": "5.0.3",
    "react-qr-code": "1.1.1",
    "react-redux": "7.2.1",
    "react-router-dom": "5.2.0",
    "react-select": "3.1.0",
    "rebass": "4.0.7",
    "redux-persist": "6.0.0",
    "redux-saga": "1.1.3",
    "styled-components": "5.2.0",
    "styled-system": "5.1.5",
    "superstruct": "0.14.2",
    "typed-react-form": "1.2.9",
    "uuid": "3.2.1",
    "uuid-by-string": "MyCryptoHQ/uuid-by-string#36afddc",
    "ws": "7.3.1",
    "yup": "0.32.9"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "eslint --fix",
      "yarn lint:styles"
    ],
    "*.json": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn tsc",
      "pre-push": "yarn test"
    }
  }
}
