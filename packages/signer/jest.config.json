{
  "displayName": "signer",
  "moduleNameMapper": {
    "fixtures": "<rootDir>/jest/__fixtures__",
    "test-utils": "<rootDir>/jest/test-utils",
    "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/jest/__mocks__/fileMock.ts",
    "\\.(css|scss)$": "<rootDir>/jest/__mocks__/styleMock.ts",
    "\\.worker.(ts|tsx)": "<rootDir>/jest/__mocks__/workerMock.js",
    "^@signer/(.+)$": "<rootDir>/../$1/src",
    "^@(.+)$": [
      "<rootDir>/node_modules/$0",
      "<rootDir>/../../node_modules/$0",
      "<rootDir>/src/$1",
      "<rootDir>/src/app/$1"
    ]
  },
  "testEnvironment": "jsdom"
}
